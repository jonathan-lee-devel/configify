{
  "name": "@jdevel/configify",
  "version": "1.0.1",
  "description": "NestJS Config on Steroids",
  "author": "Jonathan Lee <jonathan.lee.devel@gmail.com>",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathan-lee-devel/configify.git"
  },
  "keywords": [
    "nestjs",
    "config",
    "yml",
    ".env"
  ],
  "bugs": {
    "url": "https://github.com/jonathan-lee-devel/configify/issues"
  },
  "homepage": "https://github.com/jonathan-lee-devel/configify#readme",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.config.json --coverage --silent=false --verbose --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.20",
    "@nestjs/core": "^11.0.20",
    "class-validator": "^0.14.0",
    "dotenv": "^16.5.0",
    "js-yaml": "^4.1.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.20",
    "@types/jest": "29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "22.14.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8.0.0 || ^9.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^4.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^7.1.0",
    "ts-jest": "29.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
